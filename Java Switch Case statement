class Solution{
    static double switchCase(int choice, List<Double> arr){
        double res=0.0;
        switch(choice){
            case 1:
              res= arr.get(0)*arr.get(0)*3.14159265359; 
              break;
            case 2:
                res=arr.get(0)*arr.get(1);
                break;
        }
        return res;
    }
}
